<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hyy.epoxysample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EpoxySample">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".LinearLayoutManagerActivity"/>
        <activity android:name=".GridLayoutManagerActivity"/>
        <activity android:name=".LoadMoreActivity"/>
        <activity android:name=".HeaderFooterActivity"/>
        <activity android:name=".UpdateItemActivity"/>
    </application>

</manifest>